<% layout('layouts/boilerplate')%>
    <h1>All Campgrounds</h1>
    <ul>
        <% for(let c of campgrounds) { %>
            <li>
                <a href="/campgrounds/<%=c._id%>">
                    <%=c.title%> - <%=c.location%></a>
            </li>
            <h2><%=campgrounds._id%></h2>
        <%}%>
    </ul>

