<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Campgrounds</title>
</head>
<body>
    <h1>All Campgrounds</h1>
    <ul>
        <% for(let c of campgrounds) { %>
            <li>
                <a href="/campgrounds/<%=c._id%>">
                    <%=c.title%> - <%=c.location%></a>
            </li>
            <h2><%=campgrounds._id%></h2>
        <%}%>
    </ul>
</body>
</html>